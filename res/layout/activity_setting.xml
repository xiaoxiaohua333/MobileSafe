<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:huahua="http://schemas.android.com/apk/res/com.hj.mobilesafe"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TextView
        android:background="@color/blue"
        android:gravity="center"
        android:textColor="#ffffff"
        android:textSize="22sp"
        android:id="@+id/textView1"
        android:layout_width="match_parent"
        android:layout_height="55dip"
        android:text="设置中心" />
    
    
        <!-- <com.hj.mobilesafe.ui.SettingItemView
        huahua:settitle="设置自动更新"
        huahua:desc_on="设置自动更新已经开启"
        huahua:desc_off="设置自动更新已经关闭"
        android:id="@+id/siv_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        ></com.hj.mobilesafe.ui.SettingItemView> -->
        
    
    
     <com.hj.mobilesafe.ui.SettingItemView
        huahua:settitle="设置显示号码归属地"
        huahua:desc_on="设置显示号码归属地已经开启"
        huahua:desc_off="设置显示号码归属地已经关闭"
        android:id="@+id/siv_show_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        >
        
    </com.hj.mobilesafe.ui.SettingItemView>
    
      <com.hj.mobilesafe.ui.SettingItemView
          android:id="@+id/siv_callsms_safe"
        huahua:settitle="设置黑名单拦截"
        huahua:desc_on="黑名单拦截已经开启"
        huahua:desc_off="黑名单拦截已经关闭"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        >
        
    </com.hj.mobilesafe.ui.SettingItemView>
    
         <com.hj.mobilesafe.ui.SettingItemView
          android:id="@+id/siv_watchdog"
        huahua:settitle="设置程序锁"
        huahua:desc_on="程序锁已经开启"
        huahua:desc_off="程序锁已经关闭"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        >
        
    </com.hj.mobilesafe.ui.SettingItemView>
    
     
    
    
   
   
</LinearLayout>
